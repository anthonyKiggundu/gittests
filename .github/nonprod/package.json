{
  "repositoryUrl": "https://github.com/anthonyKiggundu/gittests",
  "release": {
    "branches": ["*", {"channel": "true", "prerelease": true}],
    "tagFormat": "monitor-v${version}-nonprod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anthonyKiggundu/gittestss.git",
    "directory": ".github/nonprod/"
  },
  "name": "monitorv",
  "private": true,
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "eslint",
        "releaseRules": [
          { "tag": "Breaking", "release": "major" },
          { "tag": "Build", "release": "minor" },
          { "tag": "policy", "release": "minor"},
          { "tag": "Chore", "release": "minor" },
          { "tag": "Fix", "release": "patch" },
          { "tag": "New", "release": "minor" },
          { "tag": "Update", "release": "minor" },
          { "tag": "Upgrade", "release": "minor" }
        ]
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    "@semantic-release/github",
    [
      "@semantic-release/git", {
        "assets": [".github/nonprod/package.json", "CHANGELOG.md", "*.tgz"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    [
      "@semantic-release/exec", 
      {
        "publishCmd": "git tag monitor-v${version}-prod monitor-v${version}-nonprod"
      }
    ]
  ]
}
